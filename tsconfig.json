{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "jsx": "react",
    "isolatedModules": true,
    "downlevelIteration": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "Src/*": [
        "src/*"
      ],
      "Components/*": [
        "src/components/*"
      ],
      "Utils/*": [
        "src/utils/*"
      ],
      "Pages/*": [
        "src/pages/*"
      ],
      "Routes/*": [
        "src/routes/*"
      ],
      "Api/*": [
        "src/api/*"
      ],
      "TypeRoots": [
        "node_modules/@types/*",
        "src/typings"
      ]
    },
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ]
}